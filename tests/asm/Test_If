/*************************************
*	test the use of for
**************************************/
var x=1;
print (if x>1 then true else false) + "\n";
print (if x<2 then 1 else false) + "\n";
print (if x>1 then 1 else 0) + "\n";
print (if x<=1 then 1 else 0) + "\n";
print (if x>=1 then 1 else 0) + "\n";
print (if (x+1)>=1 then 1 else 0) + "\n";
print (if (x+1)>=1 then if (x+1)<1 then 1 else 0 else 0) + "\n";
var y=15;
print (if y>10 then true else false) + "\n";
print (if y<16 then 1 else false) + "\n";
print (if y>1 then 1 else 0) + "\n";
print (if y<=1 then 1 else 0) + "\n";
print (if y>=1 then 1 else 0) + "\n";
print (if (y+1)>=1 then 1 else 0) + "\n";
print (if (y+1)>=1 then if (y+1)<1 then 1 else 0 else 0) + "\n";

