/*************************************
* Use of expressions
**************************************/
var a = -1+5;
var b = 8/4;
var c = 6*3;
var d = 6-3;
var e = 1+3-5;
var f = 8/4*9;
var g = (8/4)*9;
var h = 8/(4*9);
var i = -8*4/9;
var j = (8*4)/9;
var k = 8*(4/4);

print "Expression int \n";
print "a=" + a + "\n" + "b=" + b + + "\n";
print "c=" + c + "\n" + "d=" + d + "\n";
print "e=" + e + "\n" + "f=" + f + "\n";
print "g=" + g + "\n" + "h=" + h + "\n";
print "i=" + i + "\n" + "j=" + j + "\n";
print "k=" + k + "\n" + "\n";

print "Expression bool \n";
print (true && true) + "\n";
print (true && false)+ "\n";
print (false && false)+ "\n";
print (true || true) + "\n";
print (true || false)+ "\n";
print (false || false)+ "\n";
print (true ^^ true) + "\n";
print (true ^^ false) + "\n";
print (false ^^ false) + "\n";

print "Expression float \n";
print (12.3 + 13.6) + "\n";
print (12.3/13.6*15.2) + "\n";
print (12.3/13.6*(-15.2)) + "\n";
print (12.3+13.6*(-15.2)) + "\n";
