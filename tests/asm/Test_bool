/*************************************
* Use of bool
**************************************/
var a = true;
var b = false;
print "a=" + a + "\n";
print "b=" + b + "\n";
print "-1 ET 15" + (1 && 15)  + "\n";
print "1 ET -15" +  (1 && -15)  + "\n";
print "-17 || 0" + (-17 || 0)  + "\n";
print "0 || 0 = " + (0 || 0)  + "\n";
print "17 && (0 || 12) && -56 =" + 17 && (0 || 12) && -56;
