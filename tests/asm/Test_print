/*************************************
* Use of print expression
**************************************/
print "Texte simple" + "\n";
print "5=" + 5  + " 4=" + 4 + "\n";
print "true=" + true + "\n" + "false="  + false + "\n";
print "3.1=" + 3.1 + "\n" + "2.56="  + 2.56 + "\n";
print "[3.1]=" + [3.1] + "\n" + "[2.56]="  +  [2.56] + "\n";
print "(4+3)=" + (4+3) + "\n";
print "4.1+5.5=" + (4.1+5.5) + "\n";
print "[4.1+5.5]=" + [4.1+5.5] + "\n";